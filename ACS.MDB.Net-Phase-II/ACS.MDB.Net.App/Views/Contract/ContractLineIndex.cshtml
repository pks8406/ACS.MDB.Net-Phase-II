@model ACS.MDB.Net.App.Models.Contract

@*<script src="/Scripts/jquery.dataTables.js" type="text/javascript"></script>
<script src="/Scripts/jquery.dataTable.editTable.js" type="text/javascript"></script>*@

<table style="width: 100%">
    <tr>
        <td style="vertical-align: top; width: auto">
            <div id='div_toolbar_contractLine' class="DatatableGridButtonStyle"  style="margin-bottom:10px; margin-top:0px; "></div>
            <div id="contractLine_dialog" title="Add ContractLine"></div>
            <div id="ContractLine" style="width: auto">
                <table cellpadding="0" cellspacing="0" border="0" class="display" id="contracLine_tbl" width="100%">
                    <thead>
                        <tr>
                            <th>
                                <input type="checkbox" id="selectContractLine" />
                            </th>

                            <th>@Html.DisplayNameFor(model => model.ID) </th>
                            @*<th>@Html.DisplayNameFor(model => model.ContractLines[0].ID) </th>*@
                            <th>@Html.DisplayNameFor(model => model.ContractLines[1].ActivityCategory) </th>
                            <th>@Html.DisplayNameFor(model => model.ContractLines[2].ActivityCode) </th>
                            <th>@Html.DisplayNameFor(model => model.ContractLines[3].Account) </th>
                            <th>@Html.DisplayNameFor(model => model.ContractLines[4].JobCode) </th>
                            <th>@Html.DisplayNameFor(model => model.ContractLines[5].CostCenter) </th>
                            @*<th>@Html.DisplayNameFor(model => model.ContractLines[6].QTY) </th>*@
                            <th>@Html.DisplayNameFor(model => model.ContractLines[6].ActivityCodeId) </th>
                            <th>@Html.DisplayNameFor(model => model.ContractLines[7].ActivityCode) </th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>              
            </div>
        </td>
    </tr>
</table>